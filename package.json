{
    "name": "a-touch-of-lilac-theme",
    "displayName": "A Touch of Lilac Theme",
    "version": "1.2.2",
    "description": "Lilac tinted color UI theme with vibrant syntax highlighting colors for Visual Studio Code. It also has a No-Italics version.",
    "scripts": {
        "vsce:publish": "vsce publish"
    },
    "author": "alexnho",
    "license": "See LICENSE file",
    "publisher": "alexnho",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "lilac",
        "purple",
        "dark",
        "theme",
        "vibrant"
    ],
    "icon": "images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/alexnho/vscode-a-touch-of-lilac-theme"
    },
    "contributes": {
        "themes": [
            {
                "label": "A Touch of Lilac Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/a-touch-of-lilac.json"
